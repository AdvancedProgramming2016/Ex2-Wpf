<Window x:Class="MazeMenu.MazeWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MazeMenu"
        mc:Ignorable="d"
        Title="Maze" Height="300" Width="300">
    <Grid>
        <Button x:Name="restartButton" Content="Restart" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="74"/>
        <Button x:Name="solveButton" Content="Solve Maze" HorizontalAlignment="Left" VerticalAlignment="Top" Width="76" Margin="109,10,0,0"/>
        <Button x:Name="menuButton" Content="Main Menu" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="208,10,0,0" Click="menuButton_Click"/>

    </Grid>
</Window>
